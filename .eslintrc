{
  "root" : true,
  "extends": "eslint:recommended",
  "parser" : "babel-eslint",
  "env": {
  "node": true,
  "jasmine": true,
  "es6": true
  },
  "globals" : {
    "angular" : true,
    "device" : true,
    "modules" : true,
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false,
    "inject"     : false
  },
  "rules": {
    "strict": [0, "global"],
    "no-underscore-dangle": 0,
    "curly": 0,
    "spaced-comment": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-fallthrough": 0,
    "new-cap": 0,
    "no-loop-func": 0,
    "no-unreachable": 0,
    "no-process-exit": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-undefined" : 0,
    "no-undef" : 0,
    "no-console": 0,
    "quotes": [2, "single"]
  }
}